<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/cl_create_new_list"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/grey_white"
    tools:context=".CreateNewListActivity">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="52dp"
        android:layout_height="44dp"
        app:srcCompat="@drawable/ic_baseline_search_loop"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="114dp" />

    <EditText
        android:id="@+id/et_searchBar"
        android:layout_width="265dp"
        android:layout_height="44dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="@string/what_are_you_looking_for"
        tools:layout_editor_absoluteX="91dp"
        tools:layout_editor_absoluteY="115dp" />

    <TextView
        android:id="@+id/tv_createNewList"
        android:layout_width="270dp"
        android:layout_height="46dp"
        android:text="@string/create_new_list"
        android:textAlignment="center"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="40dp" />

    <androidx.recyclerview.widget.RecyclerView
        android:layout_width="385dp"
        android:layout_height="409dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="200dp" />
</androidx.constraintlayout.widget.ConstraintLayout>